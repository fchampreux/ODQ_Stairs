<h1>Listing roles</h1>

<table>
  <tr>
    <th>Playground</th>
    <th>Code</th>
    <th>Role</th>
    <th>Description</th>
    <th>Session number</th>
    <th>Session name</th>
    <th>Created by</th>
    <th>Updated by</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @roles.each do |role| %>
  <tr>
    <td><%= role.playground_id %></td>
    <td><%= role.code %></td>
    <td><%= role.role %></td>
    <td><%= role.description %></td>
    <td><%= role.session_number %></td>
    <td><%= role.session_name %></td>
    <td><%= role.created_by %></td>
    <td><%= role.updated_by %></td>
    <td><%= link_to 'Show', role %></td>
    <td><%= link_to 'Edit', edit_role_path(role) %></td>
    <td><%= link_to 'Destroy', role, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Role', new_role_path %>
