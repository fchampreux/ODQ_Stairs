<% provide(:title, (t('ManagingVs'))) %>

<% content_for :page_heading do %>
    <%= t('Values') %>: <%= @values_list.name%>
<% end %> 

  <ul class="mid_menu">
    <li><%= link_to t('Edit'), edit_values_list_path(@values_list) %> |</li>
    <li><%= link_to t('Up'), values_lists_path %> |</li>
    <li><%= link_to t('Destroy'), @values_list, confirm: t('Sure'), method: :delete %></li>
  </ul>

<!-- <p id="notice"><%= notice %></p> -->


      <div class="row">
        <div class="col-md-2 text-right"><%= t('Name') %>:
        </div>
        <div class="col-md-3"><%= @values_list.name%>
        </div>
        <div class="col-md-1 text-right"><%= t('Code') %>:
        </div>
        <div class="col-md-2"><%= @values_list.code%>
        </div>
        <div class="col-md-2 text-right"><%= t('UniqueId') %>:
        </div>
        <div class="col-md-1"><%= @values_list.id%>
        </div>
      </div>

      <div class="row">
        <div class="col-md-2 text-right"><%= t('Software') %>:
        </div>
        <div class="col-md-3"><%= @values_list.software.name%>
        </div>
        <div class="col-md-1 text-right"><%= t('Table') %>:
        </div>
        <div class="col-md-2"><%= @values_list.table_name%>
        </div>
      </div>

      <div class="row">
        <div class="col-md-2 text-right"><%= t('Description') %>:
        </div>
        <div class="col-md-6"><pre><%= @values_list.description%></pre>
        </div>
      </div>

      <div class="row">
        <div class="col-md-2 text-right"><%= t('Owner') %>:
        </div>
        <div class="col-md-2"><%= @values_list.owner.login%>
        </div>
        <div class="col-md-2 text-right"><%= t('CreatedBy') %>:
        </div>
        <div class="col-md-2"><%= @values_list.created_by%>
        </div>
        <div class="col-md-2 text-right"><%= t('UpdatedBy') %>:
        </div>
        <div class="col-md-2"><%= @values_list.updated_by%>
        </div>
      </div>

      <div class="row">
        <div class="col-md-2 text-right"><%= t('PlaygroundId') %>:
        </div>
        <div class="col-md-2"><%= @values_list.playground_id%>
        </div>
        <div class="col-md-2 text-right"><%= t('CreatedAt') %>:
        </div>
        <div class="col-md-2"><%= @values_list.created_at%>
        </div>
        <div class="col-md-2 text-right"><%= t('UpdatedAt') %>:
        </div>
        <div class="col-md-2"><%= @values_list.updated_at%>
        </div>
      </div>

<!-- Linked Values -->

  <div class="row">
    <div class="col-md-2">
      Download:
      <%= link_to "CSV", values_list_path(@values_list, format: "csv") %> |
      <%= link_to "Excel", values_list_path(@values_list, format: "xls") %>
    </div>
    <div class="col-md-2">
      <%= link_to t('ImportValues'), new_values_list_values_import_path(@values_list) %>
    </div>
  </div>
  
      <table class="table table-striped table-condensed">
        <tr align="left">
          <th> <%= t('Name') %> </th>
          <th> <%= t('Code') %> </th>
          <th> <%= t('Value') %> </th>
          <th> <%= t('Description') %> </th>
          <th> <%= t('UpdatedBy') %> </th>
          <th> <%= t('UpdatedAt') %> </th>
        </tr>
        <% @values_list.values.order("name ASC").each do |value| %>
        <tr align="left">
          <td valign="top"> <%=value.name %> </td>
          <td class="col_narrow"> <%=value.value_code%> </td>
          <td class="col_narrow"> <%=value.value_caption%> </td>
          <td class="col_medium"> <%=value.description%> </td>
          <td valign="top"> <%=value.updated_by%> </td>
          <td valign="top"> <%=value.updated_at%> </td>
        </tr>
        <% end%>
      </table>



