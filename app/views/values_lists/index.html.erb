<% provide(:title, 'Managing lists of values') %>

<h1>Lists of values</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Code</th>
      <th>Owner</th>
      <th>Created by</th>
      <th>Updated by</th>
    </tr>
  </thead>

  <tbody>
    <% @values_list.each do |values_list| %>
      <tr>
        <td><%= link_to values_list.name, values_list %></td>
        <td><%= values_list.description %></td>
        <td><%= values_list.code %></td>
        <td><%= values_list.owner.login %></td>
        <td><%= values_list.created_by %></td>
        <td><%= values_list.updated_by %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New List of values', new_values_list_path %>
