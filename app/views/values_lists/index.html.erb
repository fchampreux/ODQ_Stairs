<% provide(:title, (t('ManagingVs'))) %>

<% content_for :page_heading do %>
    <%=t('Values')%>
<% end %> 

<table class="table table-striped">
  <thead>
    <tr>
      <th><%=t('Name')%></th>
      <th><%=t('Description')%></th>
      <th><%=t('Code')%></th>
      <th><%=t('Owner')%></th>
      <th><%=t('UpdatedBy')%></th>
      <th><%=t('UpdatedAt')%></th>
    </tr>
  </thead>

  <tbody>
    <% @values_list.each do |values_list| %>
      <tr>
        <td><%= link_to values_list.name, values_list %></td>
        <td class="col_medium"><%= values_list.description %></td>
        <td class="col_narrow"><%= values_list.code %></td>
        <td><%= values_list.owner.login %></td>
        <td><%= values_list.updated_by %></td>
        <td><%= values_list.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to t('NewVs'), new_values_list_path %>
