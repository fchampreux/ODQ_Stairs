<!-- Tab content for Ownership -->

      <div class="row">
        <div class="col-md-2 text-right"> <%= t('UniqueId') %>:
        </div>
        <div class="col-md-2"><%= this_object.id%>
        </div>
        <div class="col-md-2 text-right"> <%= t('CreatedBy') %>:
        </div>
        <div class="col-md-2"><%= this_object.created_by%>
        </div>
      </div>
      <div class="row">
        <% if is_a? Playground %>
        <div class="col-md-2 text-right"> <%= t('PgndId') %>:
        </div>
        <div class="col-md-2"><%= this_object.playground_id%>
        </div>
        <% else %>
        <div class="col-md-4">  
        </div>
        <% end %>
        <div class="col-md-2 text-right"> <%= t('CreatedAt') %>:
        </div>
        <div class="col-md-2"><%= this_object.created_at%>
        </div>
      </div>
      <div class="row">
        <div class="col-md-2 text-right"> <%= t('Owner') %>:
        </div>
        <div class="col-md-2"><%= this_object.owner.login%>
        </div>
        <div class="col-md-2 text-right"> <%= t('UpdatedBy') %>:
        </div>
        <div class="col-md-2"><%= this_object.updated_by%>
        </div>
      </div>
      <div class="row">
        <div class="col-md-2 text-right"> <%= t('Parent') %>:
        </div>
        <div class="col-md-2"><%= # link_to this_object.parent.name, this_object.parent
        %>
        </div>
        <div class="col-md-2 text-right"> <%= t('UpdatedAt') %>:
        </div>
        <div class="col-md-2"><%= this_object.updated_at%>
        </div>
      </div>

