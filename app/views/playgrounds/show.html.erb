<% provide(:title, (t('ManagingPgs'))) %>
<% content_for :page_heading do %>
  <%= image_tag(index_audit_tag_for(@playground), :alt => "Quality hit") %> <%= t('Playground') %>: <%= @playground.name %>
<% end %>
<% content_for :breadcrums do %>
  <ul class="breadcrumbs">
    <li>> <%= @playground.name %> </li>
  </ul>
<% end %>
    
  <ul class="mid_menu">
    <li><%= link_to t('Edit'), edit_playground_path(@playground) %> |</li>
    <li><%= link_to t('Back'), playgrounds_path %> | </li>
    <li><%= link_to t('Destroy'), @playground, confirm: 'Are you sure?', method: :delete %></li>
  </ul>

<!-- <p id="notice"><%= notice %></p> -->

  <div class="tabbable">
    <ul class="nav nav-tabs">
      <li class="active"><a href="#definition" data-toggle="tab"> <%= t('Definition') %> </a></li>
      <li><a href="#ownership" data-toggle="tab"> <%= t('Ownership') %> </a></li>
    </ul>
    <div class="tab-content">

    <div class="tab-pane active" id="definition">
        <%= render partial: "shared/object_definition", locals: {this_object: @playground} %>
    </div>    

    <div class="tab-pane" id="ownership">
        <%= render partial: "shared/object_ownership", locals: {this_object: @playground} %>
    </div>
    
    </div>
  </div>

<table width=100%>
  <tr><td><hr /></td></tr>
  <tr align="left">
    <th> <%= t('Lanscapes') %> </th>
    <th></th>
  </tr>
  <tr>
    <td>
      <%= render partial: "shared/child_object_index", locals: {this_child_index: @playground.landscapes} %>
    </td>
  </tr>
  <tr>
    <td>
      <%= link_to t('NewLS'), new_playground_landscape_path(@playground) %>
    </td>
  </tr>
</table>



