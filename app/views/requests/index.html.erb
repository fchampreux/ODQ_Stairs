<h1>Listing requests</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th></th>
      <th>Who</th>
      <th></th>
      <th>When</th>
      <th></th>
      <th>Observation</th>
      <th></th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @requests.each do |request| %>
      <tr>
        <td><%= request.type %></td>
        <td><%= request. %></td>
        <td><%= request.who %></td>
        <td><%= request. %></td>
        <td><%= request.when %></td>
        <td><%= request. %></td>
        <td><%= request.observation %></td>
        <td><%= request. %></td>
        <td><%= link_to 'Show', request %></td>
        <td><%= link_to 'Edit', edit_request_path(request) %></td>
        <td><%= link_to 'Destroy', request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Request', new_request_path %>
